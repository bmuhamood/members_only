<h1>Posts</h1>

<table>
  <thead>
    <tr>
        <th>Title</th>
        <th>Body</th>
        <th>Username</th>
        <th>email</th>
        <th>Created_at</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @posts.each do |post| %>
      <tr>
          <td><%= post.title  %></td>
          <td><%= post.body  %></td>
          <td><%= hide_fields(post) %></td>
          <td><%= post.user.email %></td>
          <td><%= post.created_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= log_or_not %>
